# Configure libgpg-error.
gpg_error_cflags=""
gpg_error_libs="-lgpg-error"

PGM=libassuan-config
lib="-lassuan"
extralibs="$gpg_error_libs"
cflags=" $gpg_error_cflags"
api_version="2"
my_host="armv6l-unknown-linux-gnueabihf"
prefix=/data/data/com.termux/files/usr
exec_prefix=${prefix}
includes=""
libdirs=""
exec_prefix_set=no
echo_libs=no
echo_cflags=no
echo_prefix=no
echo_exec_prefix=no
echo_host=no


usage()
{
	cat <<EOF
Usage: $PGM [OPTIONS]
Options:
	[--prefix[=DIR]]
	[--exec-prefix[=DIR]]
	[--version]
	[--libs]
	[--cflags]
        [--host]
EOF
	exit $1
}

if test $# -eq 0; then
  usage 1 1>&2
fi

while test $# -gt 0; do
  case "$1" in
  -*=*) optarg=`echo "$1" | sed 's/[-_a-zA-Z0-9]*=//'` ;;
  *) optarg= ;;
  esac

  case $1 in
    --prefix=*)
      # Dummy
      ;;
    --prefix)
      echo_prefix=yes
      ;;
    --exec-prefix=*)
      # Dummy
      ;;
    --exec-prefix)
      echo_exec_prefix=yes
      ;;
    --variable=*)
	case "${1#*=}" in
	    prefix) echo "$prefix" ;;
	    exec_prefix) echo "$exec_prefix" ;;
	    host) echo "$my_host" ;;
	    api_version) echo "$api_version" ;;
	esac
	exit 0
	;;
    --modversion|--version)
      echo "2.5.5"
      exit 0
      ;;
    --api-version)
      echo_api_version=yes
      ;;
    --cflags)
      echo_cflags=yes
      ;;
    --libs)
      echo_libs=yes
      ;;
    --host)
      echo_host=yes
      ;;
    *)
      usage 1 1>&2
      ;;
  esac
  shift
done

if test "$echo_prefix" = "yes"; then
    echo $prefix
fi

if test "$echo_exec_prefix" = "yes"; then
    echo $exec_prefix
fi

if test "$echo_api_version" = "yes"; then
    echo $api_version
fi

if test "$echo_host" = "yes"; then
    echo "$my_host"
fi

if test "$echo_cflags" = "yes"; then
    if test "${prefix}/include" != "/usr/include" ; then
      includes="-I${prefix}/include"
      for i in $cflags ; do
	if test "$i" = "-I${prefix}/include" ; then
	  includes=""
	fi
      done
    fi
    echo $includes $cflags
fi

if test "$echo_libs" = "yes"; then
    if test "${exec_prefix}/lib" != "/usr/lib" ; then
      libdirs="-L${exec_prefix}/lib"
      for i in $lib $extralibs ; do
	if test "$i" = "-L${exec_prefix}/lib" ; then
	  libdirs=""
	fi
      done
    fi
    echo $libdirs $lib $extralibs
fi
