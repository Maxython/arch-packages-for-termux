pkgname="termux-commands"
pkgver=2021.08.11
pkgrel=3
pkgdesc="Customized termux commands."
arch=("any")
license=('MIT')
depends=('glibc')
source=("https://github.com/Maxython/arch-packages-for-termux/raw/main/$pkgname/$pkgname-$pkgver-$pkgrel-$arch.tar.xz")

build() {
	tar xJf $pkgname-$pkgver-$pkgrel-$arch.tar.xz
}

package() {
	cd $pkgname
	cp -r * $pkgdir
}
sha256sums=('f66e6af4c55a833438ed18ef0fce07f216b6aeb6db3782490ea71c52b7c03896')
