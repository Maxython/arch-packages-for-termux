
if [[ -z $* ]]; then
  echo "$0 [COMMAND]"
  echo 'The command making access to the conlib libraries.'
  echo 'glibc v2.32-2 armv7h'
  echo 'official site: https://www.gnu.org/software/libc'
  echo 'GNU site: https://www.gnu.org'
else
  if [[ -n $LD_PRELOAD ]]; then
    unset LD_PRELOAD
  fi
  export LD_LIBRARY_PATH="/data/data/com.termux/files/usr/lib"
  export LIBRARY_PATH="/data/data/com.termux/files/usr/lib"
  export SHELL="/data/data/com.termux/files/usr/bin/bashTermux"
  IFS=';' read -ra CMD <<< "$*"
  for i in "${CMD[@]}"; do
    $i
  done
fi
