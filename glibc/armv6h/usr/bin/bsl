#
#!/data/data/com.termux/files/usr/bin/bash
#

if [[ -z $* ]]; then
  echo "$0 [COMMAND]"
  echo 'The command making access to the conlib libraries.'
  echo 'glibc v2.32-2 armv6h'
  echo 'official site: https://www.gnu.org/software/libc'
  echo 'GNU site: https://www.gnu.org'
else
  export LD_LIBRARY_PATH="/data/data/com.termux/files/usr/lib"
  $*
fi
